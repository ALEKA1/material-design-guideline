<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/holder_elevation_card_container"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/default_padding_margin"
    android:orientation="vertical">

    <androidx.cardview.widget.CardView
        android:id="@+id/holder_elevation_card"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:layout_gravity="center_horizontal"
        android:layout_margin="@dimen/md_elevation_dialog"
        app:cardBackgroundColor="@color/md_white"
        app:cardElevation="@dimen/md_elevation_dialog">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/holder_elevation_tv_name"
            style="?attr/textPrimaryBody1"
            android:textColor="@color/md_pink500"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginEnd="@dimen/default_padding_margin_small"
            tools:text="name" />

    </androidx.cardview.widget.CardView>





